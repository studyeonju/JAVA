package RegularExpression;

public class ex08 {
	public static void main(String[] args) {
	String str = "hello world";
	System.out.println(str.replaceFirst("hello", "hi"));
	System.out.println(str.replaceFirst("[a-z]", "z"));
	System.out.println(str.replaceFirst("hello", "hi"));
	System.out.println(str.replaceFirst("hello", "hi"));
	}
}
