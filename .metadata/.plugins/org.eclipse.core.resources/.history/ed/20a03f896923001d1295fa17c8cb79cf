package Enum;

public class Ex02 {
	public static final String DOG ="dog";
	public static final String CAT ="cat";
	public static void main(String[] args) {
		String gender1;
		
		gender1 = Ex02.dog;
		gender1 = Ex02.cat;
		gender1="girl";
		
		Gender gender2;
		gender2 = Gender.MALE;
		gender2 = Gender.FEMALE;
		gender2="girl"; // Type mismatch: cannot convert from String to Gender
		
	}
}

enum Gender{
	MALE,FEMALE;
}
