package RegularExpression;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class ex05 {
	public static void main(String[] args) {
		String regex = "[A-Z]";
		String str1 = "xyzABC";
		String str2 = "ABC123";
		
		Pattern pattern = Pattern.compile(regex); // 패턴정의
		Matcher match1 = pattern.matcher(str1); // 매치 되는 값을 저장
		Matcher match2 = pattern.matcher(str2); // 매치 되는 값을 저장
		
		System.out.println(match1.lookingAt());
		System.out.println(match2.lookingAt());
		
	}
}
